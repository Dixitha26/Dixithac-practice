Write a program to find whether a given character is an upper,lower, number or symbols
#include<iostream>
using namespace std;
int main()
{
    char a;
    cin>>a;
    if(a>=65 && a<=90)
    {
    
        cout<<"Upper";
    }
    else if(a>=48 && a<=57)
    {
        cout<<"Number";
    }
    else if(a>=97 && a<=122)
    {
        cout<<"Lower";
    }
    else
    {
        cout<<"Symbol";
    }
}
